	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'bobjdump'",
			"cargo": {
				"args": [
					"build",
					"--bin=bobjdump",
					"--package=bobjdump"
				],
				"filter": {
					"name": "bobjdump",
					"kind": "bin"
				}
			},
			"args": [
				"./test/test.o",
				"-f"
			],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'bobjdump'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=bobjdump",
					"--package=bobjdump"
				],
				"filter": {
					"name": "bobjdump",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}